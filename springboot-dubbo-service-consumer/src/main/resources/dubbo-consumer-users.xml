<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd
		http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


	<dubbo:application name="springboot-dubbo-service-consumer" ></dubbo:application>
	<dubbo:registry address="127.0.0.1:2181" protocol="zookeeper" check="false" ></dubbo:registry>
	<dubbo:reference id="sayHiService" interface="com.anglia.li.service.SayHiService" check="true" validation="true" stub="com.anglia.li.stub.SayHiServiceStub" scope="remote"></dubbo:reference>
</beans>
